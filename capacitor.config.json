{
  "appId": "com.junxibao.healthapp",
  "appName": "紫癜精灵",
  "webDir": "www",
  "language": "zh-CN",
  "plugins": {
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon_config_sample",
      "iconColor": "#488AFF",
      "sound": "beep.wav"
    },
    "Browser": {
      "presentationStyle": "fullscreen"
    },
    "Camera": {
      "cameraPermission": "此应用需要访问相机来拍摄病例单和检查报告照片",
      "photoLibraryPermission": "此应用需要访问相册来选择病例单和检查报告照片"
    },
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#ffffff",
      "overlaysWebView": false
    },
    "CapacitorHttp": {
      "enabled": true
    },
    "App": {
      "enabled": true
    }
  },
  "server": {
    "allowNavigation": [
      "https://app.zdelf.cn"
    ],
    "cleartext": true
  }
}
